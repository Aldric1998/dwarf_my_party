<h1>New dwarf</h1>
<%= form_for(@dwarf) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :localisation %>
  <%= f.text_field :localisation %>
  <%= f.label :picture %>
  <%= f.text_field :picture %>
  <%= f.submit %>
<% end %>
