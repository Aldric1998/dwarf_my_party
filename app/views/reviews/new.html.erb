<div class ="full-container">
  <div class ="container">
      <div class="form">
        <%= simple_form_for [@offer, @review] do |f| %>
          <p> What did you think about this dwarf? Rate from 1 to 5 stars</p>
          <hr>
          <%= f.input :comment %>
            <p><%= f.input :rating %> </p>
          <%= f.submit %>
        <% end %>
      </div>
  </div>
</div>
